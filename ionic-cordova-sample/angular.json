{
    "$schema" : "./node_modules/@angular/cli/lib/config/schema.json",
    "version" : 1,
    "defaultProject" : "app",
    "newProjectRoot" : "projects",
    "projects" : {
        "app" : {
            "root" : "projects/app",
            "sourceRoot" : "projects/app/src",
            "projectType" : "application",
            "prefix" : "app",
            "architect" : {
                "build" : {
                    "builder" : "@angular-devkit/build-angular:browser",
                    "options" : {
                        "outputPath" : "www",
                        "index" : "projects/app/src/index.html",
                        "main" : "projects/app/src/main.ts",
                        "polyfills" : "projects/app/src/polyfills.ts",
                        "tsConfig" : "projects/app/tsconfig.app.json",
                        "aot" : true,
                        "assets" : [
                            {
                                "glob" : "**/*",
                                "input" : "projects/app/src/assets",
                                "output" : "assets"
                            },
                            {
                                "glob" : "**/*.svg",
                                "input" : "node_modules/ionicons/dist/ionicons/svg",
                                "output" : "./svg"
                            }
                        ],
                        "styles" : [
                            {
                                "input" : "projects/app/src/theme/variables.scss"
                            },
                            {
                                "input" : "projects/app/src/global.scss"
                            }
                        ],
                        "scripts" : []
                    },
                    "configurations" : {
                        "production" : {
                            "fileReplacements" : [
                                {
                                    "replace" : "projects/app/src/environments/environment.ts",
                                    "with" : "projects/app/src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization" : true,
                            "outputHashing" : "all",
                            "sourceMap" : false,
                            "extractCss" : true,
                            "namedChunks" : false,
                            "extractLicenses" : true,
                            "vendorChunk" : false,
                            "buildOptimizer" : true,
                            "budgets" : [
                                {
                                    "type" : "initial",
                                    "maximumWarning" : "2mb",
                                    "maximumError" : "5mb"
                                },
                                {
                                    "type" : "anyComponentStyle",
                                    "maximumWarning" : "6kb",
                                    "maximumError" : "10kb"
                                }
                            ]
                        }
                    }
                },
                "serve" : {
                    "builder" : "@angular-devkit/build-angular:dev-server",
                    "options" : {
                        "browserTarget" : "app:build"
                    },
                    "configurations" : {
                        "production" : {
                            "browserTarget" : "app:build:production"
                        }
                    }
                },
                "extract-i18n" : {
                    "builder" : "@angular-devkit/build-angular:extract-i18n",
                    "options" : {
                        "browserTarget" : "app:build"
                    }
                },
                "test" : {
                    "builder" : "@angular-devkit/build-angular:karma",
                    "options" : {
                        "main" : "projects/app/src/test.ts",
                        "polyfills" : "projects/app/src/polyfills.ts",
                        "tsConfig" : "projects/app/tsconfig.spec.json",
                        "karmaConfig" : "projects/app/karma.conf.js",
                        "assets" : [
                            {
                                "glob" : "favicon.ico",
                                "input" : "projects/app/src/",
                                "output" : "/"
                            },
                            {
                                "glob" : "**/*",
                                "input" : "projects/app/src/assets",
                                "output" : "/assets"
                            }
                        ],
                        "styles" : [],
                        "scripts" : []
                    }
                },
                "lint" : {
                    "builder" : "@angular-eslint/builder:lint",
                    "options" : {
                        "eslintConfig" : ".eslintrc.js",
                        "lintFilePatterns" : [
                            "projects/app/src/**/*.ts",
                            "projects/commons/src/**/*.ts"
                        ]
                    }
                },
                "e2e" : {
                    "builder" : "@angular-devkit/build-angular:protractor",
                    "options" : {
                        "protractorConfig" : "projects/app/e2e/protractor.conf.js",
                        "devServerTarget" : "app:serve"
                    },
                    "configurations" : {
                        "production" : {
                            "devServerTarget" : "app:serve:production"
                        }
                    }
                },
                "ionic-cordova-build" : {
                    "builder" : "@ionic/angular-toolkit:cordova-build",
                    "options" : {
                        "browserTarget" : "app:build"
                    },
                    "configurations" : {
                        "production" : {
                            "browserTarget" : "app:build:production"
                        }
                    }
                },
                "ionic-cordova-serve" : {
                    "builder" : "@ionic/angular-toolkit:cordova-serve",
                    "options" : {
                        "cordovaBuildTarget" : "app:ionic-cordova-build",
                        "devServerTarget" : "app:serve"
                    },
                    "configurations" : {
                        "production" : {
                            "cordovaBuildTarget" : "app:ionic-cordova-build:production",
                            "devServerTarget" : "app:serve:production"
                        }
                    }
                },
                "eslint" : {
                    "builder" : "@angular-eslint/builder:lint",
                    "options" : {
                        "lintFilePatterns" : [
                            "projects/app/**/*.ts",
                            "projects/app/**/*.component.html"
                        ]
                    }
                }
            }
        },
        "web" : {
            "root" : "projects/web",
            "sourceRoot" : "projects/web/src",
            "projectType" : "application",
            "prefix" : "web",
            "architect" : {
                "build" : {
                    "builder" : "@angular-devkit/build-angular:browser",
                    "options" : {
                        "outputPath" : "www",
                        "index" : "projects/web/src/index.html",
                        "main" : "projects/web/src/main.ts",
                        "polyfills" : "projects/web/src/polyfills.ts",
                        "tsConfig" : "projects/web/tsconfig.app.json",
                        "aot" : true,
                        "assets" : [
                            {
                                "glob" : "**/*",
                                "input" : "projects/web/src/assets",
                                "output" : "assets"
                            },
                            {
                                "glob" : "**/*.svg",
                                "input" : "node_modules/ionicons/dist/ionicons/svg",
                                "output" : "./svg"
                            }
                        ],
                        "styles" : [
                            {
                                "input" : "projects/web/src/theme/variables.scss"
                            },
                            {
                                "input" : "projects/web/src/global.scss"
                            }
                        ],
                        "scripts" : []
                    },
                    "configurations" : {
                        "production" : {
                            "fileReplacements" : [
                                {
                                    "replace" : "projects/web/src/environments/environment.ts",
                                    "with" : "projects/web/src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization" : true,
                            "outputHashing" : "all",
                            "sourceMap" : false,
                            "extractCss" : true,
                            "namedChunks" : false,
                            "extractLicenses" : true,
                            "vendorChunk" : false,
                            "buildOptimizer" : true,
                            "budgets" : [
                                {
                                    "type" : "initial",
                                    "maximumWarning" : "2mb",
                                    "maximumError" : "5mb"
                                }
                            ]
                        }
                    }
                },
                "serve" : {
                    "builder" : "@angular-devkit/build-angular:dev-server",
                    "options" : {
                        "browserTarget" : "web:build"
                    },
                    "configurations" : {
                        "production" : {
                            "browserTarget" : "web:build:production"
                        }
                    }
                },
                "extract-i18n" : {
                    "builder" : "@angular-devkit/build-angular:extract-i18n",
                    "options" : {
                        "browserTarget" : "web:build"
                    }
                },
                "test" : {
                    "builder" : "@angular-devkit/build-angular:karma",
                    "options" : {
                        "main" : "projects/web/src/test.ts",
                        "polyfills" : "projects/web/src/polyfills.ts",
                        "tsConfig" : "projects/web/tsconfig.spec.json",
                        "karmaConfig" : "projects/web/karma.conf.js",
                        "assets" : [
                            {
                                "glob" : "favicon.ico",
                                "input" : "projects/web/src/",
                                "output" : "/"
                            },
                            {
                                "glob" : "**/*",
                                "input" : "projects/web/src/assets",
                                "output" : "/assets"
                            }
                        ],
                        "styles" : [],
                        "scripts" : []
                    },
                    "configurations" : {
                        "ci" : {
                            "progress" : false,
                            "watch" : false
                        }
                    }
                },
                "lint" : {
                    "builder" : "@angular-devkit/build-angular:tslint",
                    "options" : {
                        "tsConfig" : [
                            "projects/web/tsconfig.app.json",
                            "projects/web/tsconfig.spec.json",
                            "projects/web/e2e/tsconfig.json"
                        ],
                        "exclude" : [
                            "**/node_modules/**"
                        ]
                    }
                },
                "e2e" : {
                    "builder" : "@angular-devkit/build-angular:protractor",
                    "options" : {
                        "protractorConfig" : "projects/web/e2e/protractor.conf.js",
                        "devServerTarget" : "web:serve"
                    },
                    "configurations" : {
                        "production" : {
                            "devServerTarget" : "web:serve:production"
                        }
                    }
                },
                "ionic-cordova-build" : {
                    "builder" : "@ionic/angular-toolkit:cordova-build",
                    "options" : {
                        "browserTarget" : "web:build"
                    },
                    "configurations" : {
                        "production" : {
                            "browserTarget" : "web:build:production"
                        }
                    }
                },
                "ionic-cordova-serve" : {
                    "builder" : "@ionic/angular-toolkit:cordova-serve",
                    "options" : {
                        "cordovaBuildTarget" : "web:ionic-cordova-build",
                        "devServerTarget" : "web:serve"
                    },
                    "configurations" : {
                        "production" : {
                            "cordovaBuildTarget" : "web:ionic-cordova-build:production",
                            "devServerTarget" : "web:serve:production"
                        }
                    }
                },
                "eslint" : {
                    "builder" : "@angular-eslint/builder:lint",
                    "options" : {
                        "lintFilePatterns" : [
                            "projects/web/**/*.ts",
                            "projects/web/**/*.component.html"
                        ]
                    }
                }
            }
        }
    },
    "cli" : {
        "defaultCollection" : "@ionic/angular-toolkit"
    },
    "schematics" : {
        "@ionic/angular-toolkit:component" : {
            "styleext" : "scss"
        },
        "@ionic/angular-toolkit:page" : {
            "styleext" : "scss"
        }
    }
}
