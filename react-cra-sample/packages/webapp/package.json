        "@ant-design/pro-descriptions": "^1.8.2",
        "@ant-design/pro-form": "^1.29.0",
        "@ant-design/pro-layout": "^6.19.5",
        "@ant-design/pro-list": "^1.9.2",
        "@ant-design/pro-skeleton": "^1.0.1",
        "@ant-design/pro-table": "^2.41.0",
        "antd": "^4.16.3"
